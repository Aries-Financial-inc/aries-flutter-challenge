{
  "version": "2.0.0",
  "tasks": [
    {
      "label": "rebuild:models",
      "type": "shell",
      "command": "flutter",
      "options": {
        "cwd": "${workspaceRoot}/app"
      },
      "args": [
        "pub",
        "run",
        "build_runner",
        "build",
        "--delete-conflicting-outputs"
      ],
      "promptOnClose": true
    },
  ]
}
